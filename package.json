{
  "name": "tonjetj-package",
  "version": "0.2.1",
  "description": "Application that tells jokes",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "test": "node src/js/joker.test.js",
    "format": "prettier -w src/**/*.js",
    "lint": "eslint src/**/*.js",
    "lint-fix": "eslint src/**/*.js --cache --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tonjetj/joker.git"
  },
  "keywords": [
    "jokes",
    "javascript",
    "application"
  ],
  "author": "Tonje T. Jenssen <tontotjen@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tonjetj/joker/issues"
  },
  "homepage": "https://github.com/tonjetj/joker#readme",
  "dependencies": {
    "build": "^0.1.4",
    "dist": "^0.1.2",
    "give-me-a-joke": "^0.5.1"
  },
  "devDependencies": {
    "eslint": "^8.26.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ]
  }
}
